(include "vehicle.ext")

{collider "airborne_medium"}
{props "airborne" "airborne_m" "radar_spo" "ir_spo" "able_paradrop" "unit"}
		;added until good air mechanics will exist
		;{props "cw_autotakeoff"}
		
{props "cw_autotakeoff_mp"} 	;to takeoff in MP when spawned

{obstacleID "plane"}
{targetClass "airborne"}
{targetSelector "cannon_bullet"}
{brain "vehicle" {state "airborne"}}
	{Weaponry
		{ManualAccuracy 1.0}	;теперь это обязательный блок
	}
{ZClipVisibility 1}
(define "chassis_defaults"
	{FuelBag
		{volume 200 }	; liters
		{rate	20 }   	; liters per kilometer
		{remain 150 }
		{fuel "fuel"}
	}
	{speed
		{Slow	5	}	; km/h
		{Normal	15	}
		{Fast	30	}
	}
)

{Chassis "wheel"
	("chassis_defaults")
	{locomotion
		{MaxSpeed 	 	30 } 
		{StartTime 		4  } 
		{BrakeTime 		1  } 
		{StopTime		2  } 
	}
		(mod "mp"
			{FuelBag
				{volume 2200 }	
				{rate	320 }   
				{remain 2000 }
				{fuel "fuel"}
			}
		)
}
{Chassis "airborne"
	("chassis_defaults")
	{locomotion
		{Maxspeed 		95 } ; kmph
 	  	{turnRadius 	40 } ; m
;		{StartTime 		6  }  ;in LP
;		{BrakeTime 		4  }  ;in LP
;		{StopTime		7  }  ;in LP
		{StartTime 		4  } 
		{BrakeTime 		6  } 
		{StopTime		10  } 
	}
(mod "mp"
			{FuelBag
				{volume 500 }	
				{rate	1 }   
				{remain 495 }
				{fuel "fuel"}
			}
		)
	{dontUseSlope}
	{ignorePatherBounds}
	{disableBackward}
	{sideSlopeSpeed 25}			
	{maxSideSlope 15}			
	{maxElevationSpeed 5}		
	{elevationAcceleration 2}
}
{chassisManager
	{current "wheel"}
}
{simulatorId "airborne"}

(define "locomotion"
	{StartTime	5	}		
	{BrakeTime	2	}		
	{StopTime	4	}		
	{SteerOn	15}			
	{SteerOff	15}         
	{SplineRadius 5}		
)

;Единый макрос для шасси
;Комментарии см в мануале

{props "cwfs_fuel"}

(define "Chassis_parameters"
     {Chassis "wheel"
	("chassis_defaults")
	{FuelBag
		{volume (* 0.5 %Fuel) }	; liters
		{rate	(* %Fuel_rate 5) }   	; liters per kilometer
		{remain (* 0.5 %Fuel_remain) }
		{fuel "fuel"}
	}
	{locomotion
		{MaxSpeed 	 	50 } 
;		{StartTime 		150 } ;in LP
;		{BrakeTime 		4 }  ;in LP
;		{StopTime		7 }  ;in LP
		{StartTime 		100 } 
		{BrakeTime 		2 } 
		{StopTime		4 } 
		{maxSpeedAtMaxTurn 	15}
		{TurnRadius		15}
		{Gears		0.2 0.3 0.5 0.8 1.0}
		{RearGears	0.05}
	}
	{disableBackward}
     }
     {Chassis "airborne"
	{FuelBag
		{volume (* 0.5 %Fuel) }	; liters
		{rate	(* %Fuel_rate 50) }   	; liters per kilometer
;		(mod "mp"
;			{rate	1 }   
;		)
		{remain (* 0.5 %Fuel_remain) }
		{fuel "fuel"}
	}
;	{speed
;		{Slow	10	}	; km/h
;		{Normal	17	}
;		{Fast	30	}
;	}
	{locomotion
		{Maxspeed 		%Speed } ; kmph
 	  	{turnRadius 	%TurnRadius } ; m
		{StartTime 		(* %Speed 0.15)  }  ;in LP
		{BrakeTime 		(* %Speed 0.05)  }  ;in LP
		{StopTime		(* %Speed 0.08)  }  ;in LP
		{StartTime 		(* %Speed 0.03)  } 
		{BrakeTime 		(* %Speed 0.08)  } 
		{StopTime		(* %Speed 0.12)  } 
	}
	{dontUseSlope}
	{ignorePatherBounds}
	{disableBackward}
;	{sideSlopeSpeed (* %Acc_hor 5)}			;degree/s
	;увеличена в 1.5 раза на MOW2
	{sideSlopeSpeed (* %Acc_hor 7.5)}			;degree/s
	{maxSideSlope (* %Maneuvrability_hor 7.5)}			;degree
;	{maxElevationSpeed (* %Acc_ver 1.5)}		;m/s
;	{elevationAcceleration (* %Maneuvrability_ver 0.8)}	
	;верт маневренность увеличена в 2 - 3 раза на MOW2
	{maxElevationSpeed (* %Acc_ver 3)}		;m/s	;увел в 2 раза
	{elevationAcceleration (* %Maneuvrability_ver 2.4)}		;увел в 3 раза
     }
     {chassisManager
	{current "wheel"}
     }
     {simulatorId "airborne"}
)

