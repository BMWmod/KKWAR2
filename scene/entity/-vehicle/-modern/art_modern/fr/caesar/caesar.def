			{RestoreIKAfterAim}
			{weapon "155mm_L52_caesar" filling "ammo bullet155_GER fg" 1}
			{gunner "gunner"}
			{charger "charger"}        	
			{foresight "foresight1"}
			("CW_stab_artillery_rt" rechargetime(14))
		}
