{Entity
	(include "/properties/tank_amph.ext")
	(include "/properties/shadow/tank.inc"  scale(1.2))
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	
	{collider "tank.light"}
	{props "light_M" "turret_rotator" "amphibia" "vision_lev04" "6_barrels_smoke"}
	{PatherID "amphibia"}
    {targetClass "light_tank"}
	
{boarder
	{anm "driver"
		{forward	{begin	"board_tank_left"}	{base "open_driver"}}
		{reverse 	{end	"emit_tank_1_hold" }{	base "open_driver" -1}}
	}
	{anm "passengers"
			{forward	{begin	"board_sd222_driver"} {base "open_aparel"}}
			{reverse 	{end	"board_sd222_driver" -1 } {base "open_aparel" -1}}
	}
}
(define "crew_human"
	{placer
		{place "%place" {group "%group"}}
	}
	{boarder
		{door "%door"}
		{link "%door" "%place" {anm "%animation"} {forward putoff} {reverse puton}}
	}
)
("crew_human" place(driver)	          door(emit1) group(crew)		          animation(driver))
("crew_human" place(gunner)           door(emit2) group(crew)	              animation(passengers))
("crew_human" place(commander)	      door(emit2) group(crew)	              animation(passengers))
("crew_human" place(commander1)	      door(emit2) group(crew)	              animation(passengers))
 	{placer
		{place "driver"	{visor "driver"}}
		{place "commander"	{visor "main"}}
		{place "commander1"	{visor "radar"}}
		
	}
	
	{extender "enumerator" {digit_folder "pla"}}
	
	("armor_IFV" turret(6 6 6 6) body(35 10 10 6))
	{volume "apploque_side1"	{Thickness 4}}
	{volume "apploque_side2"	{Thickness 4}}
	{armor
		{setting "apploque_side"
			{thickness 4}		
			{durability {blast {value 100}} {pierce {koef 999}}}
			{threshold {blast {koef 2} {value 50}}}
			{volumes "apploque_side*"}
			{health_reducing {pierce {koef 0.0001}} {blast {koef 0}} }
		}
	}

	{extender "shield"
		{on}
	}
	{component "shieldl1" {tags "shield"}} 
 	{component "shieldr1" {tags "shield"}} 
	{component "shieldl2" {tags "shield"}}
	{component "shieldr2" {tags "shield"}}
	{component "shieldl3" {tags "shield"}}
	{component "shieldr3" {tags "shield"}}
	{component "shieldl4" {tags "shield"}}
	{component "shieldr4" {tags "shield"}}
	{component "shieldl5" {tags "shield"}}
	{component "shieldr5" {tags "shield"}}

	("shield_t-64a_1981")

	{extender "inventory"
		{box
			;{item "ammo smoke_shtora" 12}	
		}
	}
	
	{Weaponry
	    {place "gun"
			{weapon "spot" filled}
			;{basic}
		}
		{place "gun2"
			{weapon "hj-10x8" filling "ammo hj-10 hj-10b" 8}
			{gunner "gunner"}
			{charger "gunner"}
			{foresight "foresight5"}
			{shooter "none"}
			{barrels 8}
			{basic}
		}
	}		
	
	{sensor
		{visor "radar"
		    {bone "Visor3"}
			{vision "radar_90grad"}	;approx value, no info
		}
		{visor "main"
		    {bone "Visor1"}
			{vision "tank_gunner_optic_IR_3g"}	;No info about the scope
		}
	}
	
	{mass 20000}	;No add ammo
	{Chassis
                ("tank_trace" fx("tracks_sm") step(0.15) len(0.45))
		{Locomotion ("CW_track_moveparam" 
			Mass(20)
			Width(3.2)
			Power(550)
			Realspeed(65)
			Gamespeed(41) 
		)}
		("CW_fuelbag" Range(500) Fuelvolume(600))
		("CW_speed_lev" Gamespeed(41))
	}
	{props "cwfs_fuel_600"}
	
	("CW_amphibia" Waterline(1.45) )
	("two_headlights")
	
	{Extension "aft-10.mdl"}

	("missile_aimpoint_tanksize")
	("cwts_buksir")
	("CW_turr_speed" deg_per_s(35))
	
}
