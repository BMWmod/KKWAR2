{Entity
	(include "/properties/armoredcar_amph.ext")
	(include "/properties/shadow/tank.inc"  scale(1.0))
	(include "/properties/selection/vehicle.inc"  scale(1.0))
	{props "vehicle" "tank" "armored_car_M" "turret" "wheeled" "with_gun" "vision_lev03" "4_wheels" "with_open_zpu" "skill_override_car"}
	{targetClass "armoredcar"}
	
	
	{boarder
			{anm "driver"
				{forward	{begin	"board_big-highcar_driver"} {end "seat_driver_car"} {base "doorleft"}}
				{reverse 	{end	"board_big-highcar_driver" -1} {base "doorleft" -1}}
			}
			{anm "gunner"
				{forward	{begin	"board_body"} {end "pose_dodge_sg2"} {base "open1"}}
				{reverse 	{end	"emit_body"} {base "open1" -1}}
			}
			{anm "passenger_l"
				{forward	{begin	"board_sd232_driver"} {end "seat_driver_car"} {base "doorleft1"}}
				{reverse 	{end	"board_sd232_driver" -1} {base "doorleft1" -1}}
			}
			{anm "passenger_r"
				{forward	{begin	"board_sd232_driver"} {end "seat_driver_car"} {base "doorright1"}}
				{reverse 	{end	"board_sd232_driver" -1} {base "doorright1" -1}}
			}
	}
	
	
	
	{placer
		{place "driver" {group "crew"} {linkbone "seat_driver"} {visor "driver"}{turnoff {sensor} {shadow} {targetable}}}
		{place "gunner" {group "crew"} {linkbone "gunner"} {turnoff {targetable 0}} {visor "mgun"} {openForCrush}}
	}
	{boarder
		{door "emit1"}
		{link "emit1" "driver" {anm "driver"} {forward putoff} {reverse puton}}
		{door "emit2"}
		{link "emit2" "gunner" {anm "gunner"} {forward putoff} {reverse puton}}
	}
(define "crew_human"
	{placer
		{place "%place" {group "%group"}}
	}
	{boarder
		{door "%door"}
		{link "%door" "%place" {anm "%animation"} {forward putoff} {reverse puton}}
	}
)


("crew_human" place(seat1)	door(emit3)  animation(passenger_l)	group(passengers))
("crew_human" place(seat2)	door(emit4)  animation(passenger_r)	group(passengers))
("crew_human" place(seat3)	door(emit3)  animation(passenger_l)	group(passengers))
("crew_human" place(seat4)	door(emit4)  animation(passenger_r)	group(passengers))
("crew_human" place(seat5)	door(emit3)  animation(passenger_l)	group(passengers))
("crew_human" place(seat6)	door(emit4)  animation(passenger_r)	group(passengers))
("crew_human" place(seat7)	door(emit3)  animation(passenger_l)	group(passengers))
("crew_human" place(seat8)	door(emit4)  animation(passenger_r)	group(passengers))
("crew_human" place(seat9)	door(emit3)  animation(passenger_l)	group(passengers))
("crew_human" place(seat10)	door(emit4)  animation(passenger_r)	group(passengers))

	("armor_light_btr" turret(3 3 3 3) body(8 8 8 6))

        ("shield_BMP2D")

	{extender "shield"
		{on}
	}
	{component "shieldl1" {tags "shield"}}
	{component "shieldl2" {tags "shield"}}
	{component "shieldr2" {tags "shield"}}





	{extender "inventory"
		{weapon "mgun"
			{mask "weapon mgun"}
		}
		{Box
			{size 8 8}
			{item "ammo 50Nato" 500}
			{item "browning_m2hb" filled {user "mgun"}}
			{item "mp_smallbox_nato" 1}
		}
	}
	{Weaponry
		{place "mgun"
			{foresight "foresight3"}
			{gunner "gunner"}
			{charger "gunner"}  
			("CW_stab_no_stabiliser")
		}
	}

	{sensor
		{visor "mgun"
			{bone "mgun"}
			{vision "vehicle_top_mgun"}
		}
	}
	{mass 13600}
	{Chassis
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("CW_wheels"
			Mass(13.6)
			Turnradius(6.2)
			Power(320)
			Realspeed(90)
			Gamespeed(51) 
			Range(1000)
			Fuelvolume(310)
		)
	}
	{props "cwfs_ext_only" "cwfs_extfuel_300"}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("two_headlights")
	{Extension "vab_reco.mdl"}
	("missile_aimpoint_trucksize")

	("cwts_buksir")
	("CW_amphibia" Waterline(1.5) )
}