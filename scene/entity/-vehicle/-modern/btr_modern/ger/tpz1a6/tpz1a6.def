{entity
	(include "/properties/armoredcar_amph.ext")
	(include "/properties/shadow/tank.inc"  scale(1.3))
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "armored_car_M" "tank" "wheeled" "vision_lev03" "amphibia" "6_wheels" "with_open_zpu"
	"sink_on_emit1" "6_barrels_smoke" "skill_override_car" "spall_liner"}
	{targetClass "armoredcar" "ap fg bullet"}
        {targetSelector "tank"}
	{PatherID "amphibia"}
	
	("armor_light_btr" turret(7 7 7 7) body(7 7 1 7))
	{volume "apploque_inter1"	{Thickness 7}}
	{armor
		{setting "apploque_inter"
			{thickness 7}		
			{durability {blast {value 100}} {pierce {koef 999}}}
			{threshold {blast {koef 2} {value 50}}}
			{volumes "apploque_inter*"}
			{health_reducing {pierce {koef 0.0001}} {blast {koef 0.0001}} }
			{fragment_pass_threshold 1}
		}
	}
	{placer
		{place "driver" {group "crew"} {visor "driver"}{linkbone "driver"}{turnoff {sensor} {shadow}}}
		{place "gunner2" {group "crew"}{linkbone "gunner2"}{turnoff {sensor} {shadow}}{openForCrush}{visor "top"}}
	}
	{boarder
		{door "emit1"}
		{link "emit1" "driver" {anm "driver"} {forward putoff} {reverse puton}}
		{door "emit2"}
		{link "emit2" "gunner2" {anm "gunner"} {forward putoff} {reverse puton}}

	}
{boarder
		{anm "driver"
			{forward	{begin	"board_big-highcar_driver"} {end "seat_driver_car"} {base "doorleft"}}
			{reverse 	{end	"board_big-highcar_driver" -1} {base "doorleft" -1}}
		}
		{anm "gunner"
			{forward	{begin	"board_big-highcar"} {end "seat_gunner_stand"} {base "doorright"}}
			{reverse 	{end	"board_big-highcar" -1} {base "doorright" -1}}
		}
		{anm "passengers"
			{forward	{begin	"board_body"}}
			{reverse 	{end	"emit_body"}}
		}

}
(define "crew_human"
	{placer
		{place "%place" {group "%group"}}
	}
	{boarder
		{door "%door"}
		{link "%door" "%place" {anm "%animation"} {forward putoff} {reverse puton}}
	}
)

