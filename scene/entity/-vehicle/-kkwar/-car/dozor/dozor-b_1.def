	("wheel" args "wheelr2")
	("wheel" args "wheell2")
	("wheel" args "wheelr3")
	("wheel" args "wheell3")
	("wheel" args "wheelr4")
	("wheel" args "wheell4")

	{extender "inventory"
		{weapon "mgun"
			{mask "weapon mgun"}
		}
		{Box
			{item "hcb" filled {user "mgun"}}
			{item "ammo hcb_ammo" 500 }
			{item "repair_kit" 1}
			{item "545x39 ammo"  600}
			{item "f1 grenade" 10}
			{item "bandage" 10 }
		}
