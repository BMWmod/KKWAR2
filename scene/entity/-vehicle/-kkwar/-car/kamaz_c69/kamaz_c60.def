	{extender "enumerator"}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	{Volume "bodyback"
		{thickness 5}
	}
	{extender "inventory"
		{box
			{item "ammo bullet57_RUS au220m ap" 73}
			{item "ammo bullet57_RUS au220m fg" 74}    

		}
	}	
	{Weaponry
		{place "gun"
			{weapon "57mm_C60" filling "ammo bullet57_RUS au220m ap"}
			{Barrels 1}
			{gunner "gunner"}
			{charger "gunner"}
			("abm_zenite")
		}
	}
	{mass 8400}


	{Chassis
		{Locomotion ("locomotion")
		{Maxspeed 70}
        {StartTime  15}
        {BrakeTime  2} ; Время торможения с макс. скорости до 0 (в секундах)
		{StopTime   5} ; Время остановки после отключения двигателя с макс. скорости до 0 (в секундах)
		{Gears   0.25 0.86 1}  ; Передаточные коэф-ты коробки передач
		{RearGears  0.11 0.37}  ; Передаточные коэф-ты коробки передач
		{GearSwitchDelay 1.5} 
	  	{maxSpeedAtMaxTurn 40}
      	{TurnRadius 6}
		}
		{FuelBag
			{volume 220 }	
			{rate	15 }   
			{remain 240 }
			{fuel "fuel"}
		}
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
