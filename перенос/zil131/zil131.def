{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(1.0))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{props "vehicle" "car" "truck" "wheeled" "6_wheels" "vision_lev03" "ural4320"}
	{able "emit_on_attack"}
	("armor_car_truck")

	{extender "enumerator"}

	{extender "shield"
			{on}
	}
	
	{boarder
		{anm "driver"
			{forward	{begin	"board_Kubel_driver"} {end "seat_driver_car"} {base "doorleft"}}
			{reverse 	{end	"board_Kubel_driver" -1} {base "doorleft" -1}}
		}
		{anm "passenger1"
			{forward	{begin	"board_Kubel_commander"} {end "seat_passenger"} {base "doorright"}}
			{reverse 	{end	"board_Kubel_commander" -1} {base "doorright" -1}}
		}
		{anm "passenger"
			{forward	{begin	"board_body"} {end "seat_passenger"}}
			{reverse 	{end	"emit_body" 1}}
		}
	}
	
("crew_human" place(driver) bone(driver) door(emit2) animation(driver) group(crew))

	
    ("crew_human" place(seat00) bone(seat00) door(emit1) animation(passenger1))
	("crew_human" place(seat01) bone(seat01) door(emit4) animation(passenger))
	("crew_human" place(seat02) bone(seat02) door(emit3) animation(passenger))
	("crew_human" place(seat03) bone(seat03) door(emit4) animation(passenger))
	("crew_human" place(seat04) bone(seat04) door(emit3) animation(passenger))
	("crew_human" place(seat05) bone(seat05) door(emit4) animation(passenger))
	("crew_human" place(seat06) bone(seat06) door(emit3) animation(passenger))


	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	{sensor
		{visor "driver"
			{bone "visor1"}
			{vision "vehicle_main"}
		}		
		
	}

	{mass 10200}

	{Chassis
		("CW_wheels"
			Mass(10.2)
			Turnradius(10)
			Power(260)
			Realspeed(100)
			Gamespeed(55) 
			Range(830)
			Fuelvolume(250)
		)
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
	}
	{props "cwfs_fuel_200"}

;	("two_headlights")
;	("cw_headlightt")
	{Extension "zil131.mdl"}
	("missile_aimpoint_trucksize")

}
